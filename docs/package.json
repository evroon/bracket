    "@types/node": "24.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "markdownlint-cli2": "^0.14.0",
    "markdownlint-rule-relative-links": "^4.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.3"
  }
